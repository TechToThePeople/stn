{
	"ui": {
		"font": "Monaco",
		"font_size": 18
	},
	"formatter": {
		"js": {
			"indent": 1,
			"indent_char": "\t"
		}
	},
    "jslint": {
        "global": [
            "window",
            "console",
            "require",
            "exports",
            "module",
            "setInterval",
            "clearInterval",
            "setTimeout",
            "clearTimeout",
            "process"
        ],
        "options": {
        	"node": true,
			"indent": 4
         }
    }
}